package singleton;

public class Sample8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

class Singleton3 {
	
	private static Singleton3 one;
	
	private Singleton3() {
	}
	
	static Singleton3 getInstance() {
		if(one == null) {
			one = new Singleton3();
		}
		return one;
	}
}
