package staticvar;


class Counter2 {
	
	static int count = 0;
	
	Counter2() {
		// static 변수이므로, this 생략해야 함
		count++;
		System.out.println(count);
	}
}

public class Sample3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Counter2 c1 = new Counter2();
		Counter2 c2 = new Counter2();
	}

}
